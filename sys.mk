# STRIP        ?= /usr/ccs/bin/strip
# LD           ?= /usr/ccs/bin/ld
# AR           ?= /usr/ccs/bin/ar
# AS           ?= /usr/ccs/bin/as
# LORDER       ?= /usr/ccs/bin/lorder
# RANLIB       ?= /usr/ccs/bin/ranlib
